{
	"name": "n8n-nodes-kafka-snappy",
	"version": "0.4.2",
	"description": "custom kafka node for n8n with snappy compression",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "",
	"author": {
		"name": "lismgmk",
		"email": "lismgmk2@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/lismgmk/n8n-nodes-kafka-custom"
	},
	"engines": {
		"node": ">=18.10",
		"pnpm": ">=9.1"
	},
	"packageManager": "pnpm@9.1.4",
	"main": "index.js",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes --write",
		"lint": "eslint nodes package.json",
		"lintfix": "eslint nodes package.json --fix",
		"prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"n8n-nodes-base/dist/credentials/Kafka.credentials.js"
		],
		"nodes": [
			"dist/nodes/KafkaSnappy/KafkaSnappy.node.js",
			"dist/nodes/KafkaSnappy/KafkaSnappyTrigger.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "^7.15.0",
		"eslint": "^8.56.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"gulp": "^4.0.2",
		"n8n-workflow": "*",
		"prettier": "^3.3.2",
		"typescript": "^5.5.3"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"@kafkajs/confluent-schema-registry": "^3.3.0",
		"kafkajs": "^2.2.4",
		"kafkajs-snappy": "^1.1.0"
	}
}
